@model IEnumerable<Domain.Models.User>

<h2>Registered Users</h2>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Registered At</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.RegisteredAt.ToString("yyyy-MM-dd HH:mm")</td>
            </tr>
        }
    </tbody>
</table>